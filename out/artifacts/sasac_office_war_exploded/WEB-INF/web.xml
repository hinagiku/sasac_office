<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">

    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>Export</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/export</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet2</servlet-name>
        <servlet-class>Import</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet2</servlet-name>
        <url-pattern>/import</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet3</servlet-name>
        <servlet-class>ShowDoc</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet3</servlet-name>
        <url-pattern>/show_doc</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet4</servlet-name>
        <servlet-class>ShowDocx</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet4</servlet-name>
        <url-pattern>/show_docx</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Servlet5</servlet-name>
        <servlet-class>Test</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet5</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>

</web-app>
